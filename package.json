{
  "name": "electron-distribution",
  "version": "1.1.1",
  "description": "Electron Distribution",
  "author": "yi-ge <a@wyr.me>",
  "scripts": {
    "dev": "nodemon src/dev.js",
    "build": "babel src -d dist",
    "start": "node dist/app.js"
  },
  "license": "MIT",
  "dependencies": {
    "@hapi/hapi": "^20.1.0",
    "@hapi/inert": "^6.0.3",
    "@hapi/vision": "^6.0.1",
    "ali-oss": "^6.13.2",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "chance": "^1.1.7",
    "cos-nodejs-sdk-v5": "^2.9.11",
    "dockerode": "^3.2.1",
    "hapi-swagger": "^14.1.0",
    "joi": "^17.4.0",
    "jssha": "^3.2.0",
    "lodash": "^4.17.21",
    "lowdb": "^1.0.0",
    "moment-timezone": "^0.5.33",
    "qiniu": "^7.3.2",
    "simple-git": "^2.36.1",
    "socket.io": "^4.0.0",
    "ssh2": "^0.8.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "nodemon": "^2.0.7"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development"
    },
    "verbose": true,
    "execMap": {
      "js": "node --harmony"
    },
    "watch": [
      "src/"
    ],
    "ext": "js,json"
  },
  "engines": {
    "node": ">= 15"
  }
}